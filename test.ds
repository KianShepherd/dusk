fn fact_tail(acc: long, n: long) -> long {
    if (n < 2l) {
        return acc;
    }
    return fact_tail(acc * n, n - 1l);
}

fn fact(n: long) -> long {
    return fact_tail(1l, n);
}

fn main(argc: float, argv: float) -> int {
    let str: string = "%ld\n";
    for (let mut i: long = 1l; i < 21l; i = i + 1l) {
        print(str, fact(i));
    }
    return 1;
}
