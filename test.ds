fn foo(n: int) -> int {
    let mut a: int = 1;
    if n <= 1 {
        a = 1;
    } else {
        a = foo(n - 1) * n;
    }
    return a;
}

fn bar(n: int) -> int {
    let mut res: int = 1;
    if (n <= 2) {
        res = 1;
    } else {
        res = bar(n - 1) + bar(n - 2);
    }

    return res;
}

fn main(argc: float, argv: float) -> int {
    printi(foo(5));
    printi(foo(10));
    printi(foo(15));
    printi(foo(20));
    return 1;
}
