fn modify() -> int* {
    let str: string = "%ld\n";
    let n: int = 10;
    let mut fact: int[10];
    for (let mut i: int = 0; i < n; i = i + 1) {
        fact[i] = 2 * i;
    }
    return fact;
}

fn modifyd() -> float* {
    let str: string = "%f\n";
    let n: int = 10;
    let mut fact: float[10];
    for (let mut i: int = 0; i < n; i = i + 1) {
        fact[i] = 2.0;
    }
    return fact;
}

fn main(argc: float, argv: float) -> int {
    let str: string = "%ld\n";
    let strf: string = "%f\n";
    let n: int = 10;
    let mut facts2: int[10];
    facts2 = modify();
    for (let mut i: int = 0; i < n; i = i + 1) {
        print(str, facts2[i]);
    }
    del(facts2);

    let mut facts2: float[10];
    facts2 = modifyd();
    for (let mut i: int = 0; i < n; i = i + 1) {
        print(strf, facts2[i]);
    }
    del(facts2);
    return 1;
}
