fn fact(n :int) -> int {
    let mut x :int = 1;
    if n <= 1 {
        x = 1;
    } else {
        x = n * fact(n - 1);
    }
    return x;
}

fn bar(n :float) -> float {
    let mut x :float = 1.0;
    if n <= 1.0 {
        x = 1.0;
    } else {
        x = n * bar(n - 1.0);
    }
    return x;
}

fn main(argc: float, argv: float) -> int {
    for (let mut a: int = 1; a < 10; a = a +1) {
        printi(fact(a));
    }
    for (let mut a: float= 1.0; a < 10.0; a = a +1.0) {
        printd(bar(a));
    }
    return 1;
}
